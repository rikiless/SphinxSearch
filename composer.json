{
	"name": "rikiless/sphinx-search",
	"description": "Simple handler to query sphinx search",
	"keywords": [
		"sphinx",
		"search",
		"fulltext",
		"nette",
		"api"
	],
	"require": {
		"php": ">=5.4",
		"sphinx-search/client": "*"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "sphinx-search/client",
				"version": "2.0.1",
				"source": {
					"url": "http://sphinxsearch.googlecode.com/svn/",
					"type": "svn",
					"reference": "tags/REL_2_0_1/api"
				},
				"autoload": {
					"classmap": ["sphinxapi.php"]
				}
			}
		}
	],
	"minimum-stability": "dev",
	"license": "MIT",
	"authors": [
		{
			"name": "Richard Tekel",
			"email": "richardtekel@me.com",
			"homepage": "http://www.rikiho.net"
		}
	],
	"autoload": {
		"psr-0": {
			"Rikiless\\": "src/"
		},
		"classmap": [
			"src/Rikiless/Sphinx/exceptions.php"
		]
	}
}
